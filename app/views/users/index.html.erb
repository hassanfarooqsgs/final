<h1>List of Users</h1>
<table class="table table-striped">
  <thead>
    <tr>
      <th>User ID</th> <br/>
      <th>FirstName</th><br/>
      <th>MiddleName</th>
      <th>LastName</th>
      <th>Email</th>
      <th>Contact</th>
      <th>Country</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.id %></td>
        <td><%= user.first_name %></td>
        <td><%= user.middle_name %></td>
        <td><%= user.last_name %></td>
        <td><%= user.email %></td>
        <td><%= user.phone_number %></td>
        <td><%= user.country %></td>
      </tr>
    <% end %>
  </tbody>
</table>
